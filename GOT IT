<html>
  <body>
    <label>Hi Eddie!</label>
    
    <input type="text" id="mytextbox" />
  <button onclick=buttonClicked()>Click</button>
  </body>
  <script type="text/javascript">
    
    
    function buttonClicked() {
      var mytext = document.getElementById("mytextbox");
      alert(mytext.value);
 }
  </script>
</html>
